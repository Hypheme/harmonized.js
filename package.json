{
  "name": "harmonized",
  "version": "0.0.0",
  "description": "state, storage and server manager for react and react-native based on mobx.js",
  "main": "lib/harmonized.js",
  "umd:main": "lib/harmonized.umd.js",
  "module": "lib/harmonized.module.js",
  "jsnext:main": "lib/harmonized.module.js",
  "scripts": {
    "codecov": "cat coverage/*/lcov.info | codecov",
    "integration": "NODE_ENV=test ./node_modules/karma/bin/karma start integration.karma.conf.js --auto-watch --no-single-run",
    "integration:travis": "NODE_ENV=test ./node_modules/karma/bin/karma start integration.karma.conf.js",
    "test:travis": "NODE_ENV=test ./node_modules/karma/bin/karma start",
    "test": "NODE_ENV=test ./node_modules/karma/bin/karma start --auto-watch --no-single-run",
    "eslint": "./node_modules/.bin/eslint src/**",
    "prepublish": "yarn build",
    "build": "rimraf lib && yarn build:bundles && yarn build:bundles:min",
    "build:bundles": "rollup -c",
    "build:bundles:min": "rollup -c --environment MINIFY"
  },
  "repository": {
    "type": "git",
    "url": "git+https://JohannesMerz@github.com/Hypheme/harmonized.js.git"
  },
  "keywords": [
    "react",
    "react-native",
    "mobx",
    "indexdDB",
    "SQLite",
    "REST"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hypheme/harmonized.js/issues"
  },
  "homepage": "https://github.com/Hypheme/harmonized.js#readme",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-template": "^6.16.0",
    "babel-types": "^6.21.0",
    "body-parser": "^1.15.1",
    "es6-promise": "^4.0.5",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "express": "^4.13.4",
    "fetch-mock": "^5.12.1",
    "flow-bin": "^0.57.3",
    "jasmine-ajax": "^3.2.0",
    "jasmine-core": "^2.5.2",
    "jasmine-promises": "^0.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.4.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-diff-reporter": "^1.1.1",
    "karma-jasmine-matchers": "^3.0.1",
    "karma-rollup-preprocessor": "^5.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.5",
    "mobx": "^3.3.1",
    "nock": "^9.0.2",
    "proxyquire": "^1.7.9",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-gzip": "^1.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "glob-to-regexp": "^0.3.0",
    "lodash": "^4.17.4",
    "uuid": "^3.0.1"
  },
  "peerDependencies": {
    "mobx": "^3.3.1"
  }
}
