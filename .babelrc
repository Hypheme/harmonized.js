{
  "presets": [
    "react",
    "es2015",
    "stage-1"
  ],
  "plugins": [
    "transform-flow-strip-types",
    "es6-promise",
    "transform-decorators-legacy"
  ],
  "env": {
    "test": {
      "plugins": [
        ["istanbul", { "exclude": ["src/**/*.spec.js", "test/**/*.js"] }],
        "rewire"
      ],
    }
  }
}
